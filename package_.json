{
  "name": "remove-error-lines",
  "publisher": "Stan",
  "displayName": "Remove Error Lines",
  "description": "An extension to remove lines with errors from code",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.removeLinesWithErrors"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.removeLinesWithErrors",
        "title": "Remove Lines With Errors"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.removeLinesWithErrors",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^4.5.2",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
